<!-- map of group/user names to permissions -->
<!-- * indicates anyone has a permission -->
<!-- - indicates no one has a permission -->


<permissionMap>

<permission name="adminJobs">
	<group name="administrators"/>
</permission>
<permission name="adminModules">
	<group name="administrators"/>
</permission>
<permission name="adminSuites">
	<group name="administrators"/>
</permission>
<permission name="adminPipelines">
	<group name="administrators"/>
</permission>
<permission name="adminServer">
	<group name="administrators"/>
</permission>

<permission name="createModule">
	<group name="administrators"/>
</permission>

<permission name="createPrivatePipeline">
	<group name="*"/>
</permission>

<permission name="createPrivateSuite">
	<group name="*"/>
</permission>

<permission name="createPublicPipeline">
	<group name="*"/>
</permission>

<permission name="createPublicSuite">
	<group name="*"/>
</permission>

</permissionMap>