<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="org.genepattern.server.domain">
  <class name="Props" table="PROPS" optimistic-lock="none">
    <id name="key" type="string" unsaved-value="null">
      <column name="`KEY`" not-null="true" unique="true" index="SYS_PK_52" length="100"/>
      <generator class="assigned"/>
    </id>
    <property name="value" type="string" column="VALUE" length="256"/>
  </class>
</hibernate-mapping>
